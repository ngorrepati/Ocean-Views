{
    "manifest_version": 2,
    "name": "Ocean Views",
    "description": "Whenever a new tab is opened, a breathtaking ocean view and
     a inspirational quote appear!",
    "version": "1.0",
    "permissions": ["activeTab"],
    "author": "Nikhil Gorrepati",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Enjoy the view!"
    },

    "icons": {
        "128": "icon.png"
    },

    "chrome_url_overrides": {
        "newtab": "ocean.html"
    }
}
